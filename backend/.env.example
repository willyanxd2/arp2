# Server Configuration
PORT=3001
NODE_ENV=production

# Database
DB_PATH=./data/arp_monitoring.db

# ARP Scan Configuration
DEFAULT_INTERFACE=eth0
DEFAULT_SUBNET=192.168.1.0/24
SCAN_TIMEOUT=30

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log